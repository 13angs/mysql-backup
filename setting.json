{
    "servers": [
        {
            "name": "eng-api-db",
            "port": 3306,
            "databases": [
                "EngCartService",
                "EngUserService"
            ]
        }
    ],
    "actions": [
        "backup",
        "restore",
        "logging"
    ],
    "dest": {
        "single_backup": "./data/backup/single",
        "multiple_backup": "./data/backup/multiple",
        "backup": "./data/backup",
        "restore": "./data/restore",
        "logs": "./data/logs"
    },
    "backup_type": [
        "single",
        "multiple"
    ]
}